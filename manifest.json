{
    "manifest_version": 3,
    "name": "Panopto Volume Boost",
    "version": "0.1",
    "description": "Extension to boost volume of Panopto videos",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["https://au.cloud.panopto.eu/*"],
        "js": ["content.js"],
        "css": ["content.css"]
      }
    ]
  }